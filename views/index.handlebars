<div class="card text-center">
  <div class="card-header">
  </div>
  <div class="card-body">
    <h1 class="card-title">Gamer Scrape</h1>
    <p class="card-text text-uppercase">All your games r belong to me...</p>
  </div>
  <div class="card-footer text-muted">
  </div>
</div>


<div id="wrapper">
  <h1>Scraper's Weekly Articles</h1>
  <h2><i>Click an article to make notes</i></h2>
  <div class="list-group" id="articles">
    {{#each articles}}
    <p data-id="{{this._id}}" class="list-group-item">{{this.title}}<br>
      <a href="{{this.link}}">{{this.link}}</a>
    </p>
    {{/each}}
  </div>


  <div id="notes">
  </div>

</div>
<script src=" https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"> </script>
<script src="app.js">
</script>